{
  "plugins": ["import"],
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings"],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [["builtin", "external"], ["internal"], ["sibling", "parent", "index"]],
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
